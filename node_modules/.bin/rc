../rc/cli.js
=======
#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../rc/cli.js" "$@"
  ret=$?
else 
  node  "$basedir/../rc/cli.js" "$@"
  ret=$?
fi
exit $ret
