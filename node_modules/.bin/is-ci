esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../is-ci/bin.js" "$@"
  ret=$?
else 
  node  "$basedir/../is-ci/bin.js" "$@"
  ret=$?
fi
exit $ret
